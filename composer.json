{
    "name": "phlexible/node-connection-bundle",
    "description": "phlexible node connection bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["connection"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\NodeConnectionBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "phlexible/element-bundle": "~1.3",
        "phlexible/gui-bundle": "~1.3",
        "phlexible/tree-bundle": "~1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.6",
        "symfony/config": "~2.8",
        "symfony/dependency-injection": "~2.8",
        "symfony/event-dispatcher": "~2.8",
        "symfony/http-kernel": "~2.8"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
